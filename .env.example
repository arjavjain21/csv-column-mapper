# Supabase Configuration
# Get these from your Supabase project settings: https://app.supabase.com/project/_/settings/api
PUBLIC_SUPABASE_URL=your-supabase-project-url
PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Stripe Configuration
# Get these from your Stripe dashboard: https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Stripe Price IDs (create these in Stripe Products)
STRIPE_PRICE_PRO_MONTHLY=price_your-pro-monthly-price-id
STRIPE_PRICE_PRO_YEARLY=price_your-pro-yearly-price-id
STRIPE_PRICE_BUSINESS_MONTHLY=price_your-business-monthly-price-id
STRIPE_PRICE_BUSINESS_YEARLY=price_your-business-yearly-price-id
STRIPE_PRICE_LIFETIME=price_your-lifetime-price-id

# Email Configuration (Resend)
RESEND_API_KEY=re_your-resend-api-key
EMAIL_FROM=noreply@mapcsv.com

# App Configuration
PUBLIC_APP_URL=http://localhost:5173
NODE_ENV=development
PORT=3000
